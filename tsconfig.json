{
  "compilerOptions": {
    "removeComments": true,
    "sourceMap": true,
    "locale": "en-ca",
    "target": "es6",
    "skipLibCheck": true,
    "declaration": false,
    "strictNullChecks": true,
    "noImplicitUseStrict": false,
    "allowUnreachableCode": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "jsx": "react",
    "lib": ["es2015", "es2017", "dom"]
  },
  "exclude": ["node_modules", "dist"]
}
