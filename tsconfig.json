{
  "compilerOptions": {
    "removeComments": true,
    "sourceMap": true,
    "locale": "en-ca",
    "target": "es5",
    "declaration": false,
    "noImplicitUseStrict": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "public"
  ]
}
